<div class= "jumbotron text-center" style="height: 220px"><h1>Sign Up For a New Account </h1></div>

  <%= form_for @user, url: { action: "create" } do |f| %>
  <label style="color:#e3e2df">Name:
  <%= f.text_field :name %></label>
  <label style="color:#e3e2df">Password:
  <%= f.text_field :password %></label>
  <label style="color:#e3e2df">Picture:
  <%= f.file_field :avatar %></label>


